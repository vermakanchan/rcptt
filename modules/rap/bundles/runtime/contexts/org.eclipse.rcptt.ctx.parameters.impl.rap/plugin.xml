<plugin>
   <extension
         point="org.eclipse.rcptt.core.contextProcessors">
      <processor
            class="org.eclipse.rcptt.ctx.parameters.impl.rap.ParametersContextProcessor"
            type="org.eclipse.rcptt.ctx.parameters">
      </processor>
   </extension>
   <extension
         point="org.eclipse.rcptt.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.rcptt.ctx.parameters.impl.rap.ResetParamsService"
            name="ResetParams"
            namespace="http://eclipse.org/rcptt/ctx/parameters">
      </scriptlet>
   </extension>
   <extension
         point="org.eclipse.rcptt.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.rcptt.ctx.parameters.impl.rap.SetParamService"
            name="SetParam"
            namespace="http://eclipse.org/rcptt/ctx/parameters">
      </scriptlet>
   </extension>
   <extension
         point="org.eclipse.rcptt.ecl.core.scriptlet">
      <scriptlet
            class="org.eclipse.rcptt.ctx.parameters.impl.rap.GetParamService"
            name="GetParam"
            namespace="http://eclipse.org/rcptt/ctx/parameters">
      </scriptlet>
   </extension>

</plugin>
